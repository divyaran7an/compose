{
  "name": "vercel-kv",
  "displayName": "Vercel KV Integration",
  "description": "Vercel KV (Redis) integration for key-value storage, caching patterns, and real-time data management",
  "packages": [
    { "name": "@vercel/kv", "version": "^1.0.0" },
    { "name": "next", "version": "^14.0.0" },
    { "name": "react", "version": "^18.0.0" },
    { "name": "react-dom", "version": "^18.0.0" },
    { "name": "class-variance-authority", "version": "^0.7.1" },
    { "name": "clsx", "version": "^2.0.0" },
    { "name": "tailwind-merge", "version": "^2.0.0" },
    { "name": "lucide-react", "version": "^0.263.1" },
    { "name": "@radix-ui/react-slot", "version": "^1.1.0" },
    { "name": "@radix-ui/react-tabs", "version": "^1.1.0" },
    { "name": "@radix-ui/react-separator", "version": "^1.1.0" },
    { "name": "@radix-ui/react-label", "version": "^2.1.0" }
  ],
  "devPackages": [
    { "name": "@types/node", "version": "^20.0.0" },
    { "name": "@types/react", "version": "^18.0.0" },
    { "name": "@types/react-dom", "version": "^18.0.0" },
    { "name": "typescript", "version": "^5.0.0" },
    { "name": "tailwindcss", "version": "^3.3.0" },
    { "name": "tailwindcss-animate", "version": "^1.0.7" },
    { "name": "autoprefixer", "version": "^10.4.16" },
    { "name": "postcss", "version": "^8.4.31" }
  ],
  "envVars": [
    { "name": "KV_URL", "description": "Vercel KV database URL", "required": true },
    { "name": "KV_REST_API_URL", "description": "Vercel KV REST API URL", "required": true },
    { "name": "KV_REST_API_TOKEN", "description": "Vercel KV REST API token for read/write operations", "required": true },
    { "name": "KV_REST_API_READ_ONLY_TOKEN", "description": "Vercel KV REST API read-only token", "required": false }
  ],
  "files": {
    "example.tsx": "src/pages/vercel-kv.tsx",
    "kv-config.ts": "src/lib/vercel-kv/config.ts",
    "kv-operations.ts": "src/lib/vercel-kv/operations.ts",
    "kv-cache.ts": "src/lib/vercel-kv/cache.ts",
    "kv-realtime.ts": "src/lib/vercel-kv/realtime.ts",
    "kv-index.ts": "src/lib/vercel-kv/index.ts",
    "KVExample.tsx": "src/components/KVExample.tsx",
    "CacheExample.tsx": "src/components/CacheExample.tsx",
    "LeaderboardExample.tsx": "src/components/LeaderboardExample.tsx",
    "README.md": "README.md"
  }
} 