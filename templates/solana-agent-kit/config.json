    { "name": "autoprefixer", "version": "^10.4.16" },
    { "name": "postcss", "version": "^8.4.31" }
  ],
  "envVars": [
    {
      "name": "OPENAI_API_KEY",
      "description": "OpenAI API key for the Solana AI agent"
    },
    {
      "name": "RPC_PROVIDER_URL",
      "description": "Solana RPC endpoint URL (e.g., https://api.devnet.solana.com for devnet)"
    },
    {
      "name": "SOLANA_PRIVATE_KEY",
      "description": "Your Solana private key in base58 format (keep this secure!)"
    }
  ],
  "files": {
    "example.tsx": "src/pages/solana-agent-kit.tsx",
    "api-solana-agent.ts": "src/pages/api/solana-agent.ts",
